


















/*每个页面公共css */
.bb-container{
	width:100%;
	height: 100vh;
	box-sizing: border-box;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
}
.bb-content{
	width:100%;
	-webkit-box-flex:1;
	-webkit-flex:1;
	        flex:1;
	overflow: hidden;
	overflow-y: auto;
}

